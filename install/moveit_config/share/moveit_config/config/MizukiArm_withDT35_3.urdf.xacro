<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="MizukiArm_withDT35_3">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import MizukiArm_withDT35_3 urdf file -->
    <xacro:include filename="$(find mizukiArm_withDT35_3)/urdf/MizukiArm_withDT35_3.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="MizukiArm_withDT35_3.ros2_control.xacro" />

    <xacro:MizukiArm_withDT35_3_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
